<h1>Editing tweet</h1>

<% form_for(@tweet) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :group_id %><br />
    <%= f.text_field :group_id %>
  </p>
  <p>
    <%= f.label :parent_id %><br />
    <%= f.text_field :parent_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :tweet %><br />
    <%= f.text_area :tweet %>
  </p>
  <p>
    <%= f.label :tweet_lat %><br />
    <%= f.text_field :tweet_lat %>
  </p>
  <p>
    <%= f.label :tweet_lon %><br />
    <%= f.text_field :tweet_lon %>
  </p>
  <p>
    <%= f.label :children_cnt %><br />
    <%= f.text_field :children_cnt %>
  </p>
  <p>
    <%= f.label :picture_path %><br />
    <%= f.text_field :picture_path %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @tweet %> |
<%= link_to 'Back', tweets_path %>
